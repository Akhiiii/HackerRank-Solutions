#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int t;
    cin>>t;
    while(t--)
    {
        int a,b;
        cin>>a>>b;
        int r = ceil(sqrt(a));
        int count = 0;
        while(r*r <= b)
        {
            count++;
            r++;
        }
        cout<<count<<endl;
    }
    return 0;
}
